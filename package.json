{
  "name": "telegram-daily-digest",
  "version": "1.0.0",
  "description": "Интеллектуальный телеграм бот для агрегации и анализа контента из телеграм каналов",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "docker:build": "docker build -t telegram-daily-digest .",
    "docker:run": "docker run -p 3000:3000 telegram-daily-digest",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "userbot:test": "tsx src/userbot/test-connection.ts",
    "monitoring:test": "tsx src/test-monitoring.ts",
    "sessions:list": "tsx src/userbot/session-cli.ts list",
    "sessions:delete": "tsx src/userbot/session-cli.ts delete",
    "sessions:cleanup": "tsx src/userbot/session-cli.ts cleanup",
    "sessions:generate": "tsx src/userbot/session-cli.ts generate",
    "ai:test": "tsx src/test-ai-analysis.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "news",
    "digest",
    "ai",
    "typescript",
    "telethon"
  ],
  "author": "Vital Yudin",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/pg": "^8.15.5",
    "axios": "^1.6.2",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.63.0",
    "openai": "^4.20.1",
    "pg": "^8.16.3",
    "telegram": "^2.26.22",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.9",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.61.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
